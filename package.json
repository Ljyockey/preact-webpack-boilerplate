{
  "name": "webpack-example-nasa-apod",
  "version": "1.0.0",
  "description": "**ABOUT**",
  "main": "index.js",
  "scripts": {
    "build": "node scripts/build.js",
    "test": "jest"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "7.1.2",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "preact": "8.2.9",
    "preact-compat": "3.18.4",
    "preact-render-spy": "1.3.0",
    "webpack": "3.8.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/components/*.{js,jsx}",
      "src/index.jsx"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "modulePaths": [
      "src/"
    ],
    "moduleNameMapper": {
      "^react$": "preact-compat",
      "^react-native$": "react-native-web",
      "^react-dom/server$": "<rootDir>/node_modules/preact-render-to-string/dist/index.js",
      "^react-dom/test-util$": "<rootDir>/node_modules/preact-test-utils/lib/index.js",
      "^react-test-renderer/shallow$": "<rootDir>/node_modules/preact-test-utils/lib/index.js",
      "^react-test-renderer$": "<rootDir>/node_modules/preact-test-utils/lib/index.js",
      "^react-addons-test-utils$": "<rootDir>/node_modules/preact-test-utils/lib/index.js",
      "^smartstream-widgets[/](.+)": "<rootDir>/widgets/src/main/javascript/$1",
      "^mock-data[/](.+)": "<rootDir>/mock-data/$1",
      "^styles[/](.+)": "<rootDir>/widgets/src/main/resources/styles/$1",
      "^images[/](.+)": "<rootDir>/widgets/src/main/resources/images/$1",
      "^resources[/](.+)": "<rootDir>/widgets/src/main/resources/$1"
    }
  }
}
